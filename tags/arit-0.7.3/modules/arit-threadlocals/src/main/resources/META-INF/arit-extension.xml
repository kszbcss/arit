<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:core="http://code.google.com/p/arit/core"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
                           http://code.google.com/p/arit/core https://arit.googlecode.com/svn/schemas/core.xsd">
    <core:resource-type identifier="threadlocal"/>
    <bean class="com.googlecode.arit.threadlocals.ThreadLocalEnumeratorFactory"/>
    <bean class="com.googlecode.arit.threadlocals.ThreadLocalInspector"/>
    <bean class="com.googlecode.arit.threadlocals.ThreadLocalValueInspector"/>
    <bean class="com.googlecode.arit.threadlocals.harmony.HarmonyThreadLocalInspectorPlugin"/>
    <bean class="com.googlecode.arit.threadlocals.sun.SunThreadLocalInspectorPlugin"/>
</beans>