<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
    <bean class="com.googlecode.arit.ResourceTypeFactory">
        <qualifier value="jdbc-driver"/>
        <property name="iconFormat" value="GIF"/>
        <property name="resource" value="com/googlecode/arit/jdbc/icon.gif"/>
        <property name="identifier" value="jdbc-driver"/>
    </bean>
    <bean class="com.googlecode.arit.jdbc.DriverManagerInspector"/>
    <bean class="com.googlecode.arit.jdbc.JdbcDriverEnumeratorFactory"/>
    <bean class="com.googlecode.arit.jdbc.harmony.HarmonyDriverManagerInspectorPlugin"/>
    <bean class="com.googlecode.arit.jdbc.sun.SunDriverManagerInspectorPlugin"/>
</beans>