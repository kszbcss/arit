<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:core="http://code.google.com/p/arit/core"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
                           http://code.google.com/p/arit/core https://arit.googlecode.com/svn/schemas/core.xsd">
    <core:resource-type identifier="mbean" iconFormat="GIF" resource="com/googlecode/arit/mbeans/icon.gif"/>
    <bean class="com.googlecode.arit.mbeans.MBeanEnumeratorFactory"/>
    <bean class="com.googlecode.arit.mbeans.MBeanServerInspector"/>
    <bean class="com.googlecode.arit.mbeans.mx4j.MX4JMBeanServerInspectorPlugin"/>
    <bean class="com.googlecode.arit.mbeans.sun.java5.SunJava5MBeanServerInspectorPlugin"/>
    <bean class="com.googlecode.arit.mbeans.sun.java6.SunJava6MBeanServerInspectorPlugin"/>
</beans>