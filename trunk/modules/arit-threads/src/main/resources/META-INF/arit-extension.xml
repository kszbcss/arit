<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:core="http://code.google.com/p/arit/core"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
                           http://code.google.com/p/arit/core https://arit.googlecode.com/svn/schemas/core.xsd">
    <core:resource-type identifier="acceptor-thread"/>
    <core:resource-type identifier="threadgroup"/>
    <core:resource-type identifier="thread"/>
    <core:resource-type identifier="timerthread"/>
    <bean class="com.googlecode.arit.threads.AcceptorThreadInspectorPlugin"/>
    <bean class="com.googlecode.arit.threads.DefaultThreadInspectorPlugin"/>
    <bean class="com.googlecode.arit.threads.ThreadEnumeratorFactory"/>
    <bean class="com.googlecode.arit.threads.ThreadGroupEnumeratorFactory"/>
    <bean class="com.googlecode.arit.threads.ThreadInspector"/>
    <bean class="com.googlecode.arit.threads.harmony.HarmonyTimerThreadInspectorPlugin"/>
    <bean class="com.googlecode.arit.threads.sun.SunTimerThreadInspectorPlugin"/>
</beans>