<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:core="http://code.google.com/p/arit/core"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
                           http://code.google.com/p/arit/core https://arit.googlecode.com/svn/schemas/core.xsd">
    <core:resource-type identifier="shutdown-hook" iconFormat="GIF" resource="com/googlecode/arit/shutdown/icon.gif"/>
    <bean class="com.googlecode.arit.shutdown.ShutdownHookEnumeratorFactory"/>
    <bean class="com.googlecode.arit.shutdown.ShutdownHookInspector"/>
    <bean class="com.googlecode.arit.shutdown.harmony.HarmonyShutdownHookInspectorPlugin"/>
    <bean class="com.googlecode.arit.shutdown.ibm.IBMShutdownHookInspectorPlugin"/>
    <bean class="com.googlecode.arit.shutdown.sun.SunShutdownHookInspectorPlugin"/>
</beans>