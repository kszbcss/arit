<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
    version="2.5">
    <display-name>Inspector</display-name>
    <servlet>
        <display-name>Inspector servlet</display-name>
        <servlet-name>InspectorServlet</servlet-name>
        <servlet-class>com.google.code.rex.servlet.InspectorServlet</servlet-class>
    </servlet>
    <servlet>
        <display-name>Test servlet</display-name>
        <servlet-name>TestServlet</servlet-name>
        <servlet-class>com.google.code.rex.servlet.TestServlet</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet-mapping>
        <servlet-name>InspectorServlet</servlet-name>
        <url-pattern>/</url-pattern>
    </servlet-mapping>
</web-app>
